{
  "name": "soundclash-front",
  "version": "0.1.0",
  "description": "A React SPA for soundclash",
  "main": "index.html",
  "config": {
    "dist": "./dist",
    "src": "./src"
  },
  "scripts": {
    "build:bundle": "webpack --env.prod --progress",
    "watch:bundle": "webpack-dev-server --env.dev --hot --watch --progress",
    "clean": "rimraf $npm_package_config_dist",
    "start": "npm run clean & run-p -n watch:*",
    "build": "npm run clean & run-p -n build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-pearce/soundclash-front.git"
  },
  "author": "Chris Pearce <crite70@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chris-pearce/soundclash-front/issues"
  },
  "homepage": "https://github.com/chris-pearce/soundclash-front#readme",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-hot-loader": "^1.1.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "uuid": "^3.0.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.3"
  }
}
